#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config EXAMPLES_ELTRES_CALC_DISTANCE
	tristate "ELTRES calc distance application"
	depends on EXTERNALS_ELTRES
	default n
	---help---
		Enable the eltres_calc_distance app

if EXAMPLES_ELTRES_CALC_DISTANCE

config EXAMPLES_ELTRES_CALC_DISTANCE_PROGNAME
	string "Program name"
	default "eltres_calc_distance"
	---help---
		This is the name of the program that will be use when the NSH ELF
		program is installed.

config EXAMPLES_ELTRES_CALC_DISTANCE_PRIORITY
	int "eltres_calc_distance task priority"
	default 100

config EXAMPLES_ELTRES_CALC_DISTANCE_STACKSIZE
	int "eltres_calc_distance stack size"
	default DEFAULT_TASK_STACKSIZE

endif
